{
  "name": "@central-de-vagas/backend.reporter-service",
  "scripts": {
    "start": "bun run src/api/main.ts",
    "dev": "bun run --watch src/api/main.ts --hot",
    "lint:check": "biome lint .",
    "lint:fix": "biome lint --unsafe --write .",
    "format:check": "biome format .",
    "format:fix": "biome format --write .",
    "code:check": "biome check . && bun run tsc:check",
    "code:fix": "biome check --unsafe --write .",
    "tsc:check": "tsc --noEmit",
    "tsc:watch": "tsc --noEmit --watch",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@date-fns/tz": "^1.2.0",
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0",
    "date-fns": "^4.1.0",
    "express": "^4.21.2",
    "jsreport-chrome-pdf": "^1.10.0",
    "jsreport-core": "^2.10.1",
    "jsreport-handlebars": "^2.1.0",
    "libphonenumber-js": "^1.11.20",
    "lodash-es": "^4.17.21",
    "minio": "^8.0.4",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "p-map": "^7.0.3",
    "p-mutex": "^0.1.0",
    "p-queue": "^8.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.15",
    "type-fest": "^4.35.0",
    "winston": "^3.17.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bun": "latest",
    "@types/express": "^5.0.0",
    "@types/jsreport-chrome-pdf": "^1.6.6",
    "@types/jsreport-core": "^2.0.9",
    "@types/jsreport-handlebars": "^2.1.3",
    "@types/lodash-es": "^4.17.12",
    "@types/minio": "^7.1.1",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  }
}
