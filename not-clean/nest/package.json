  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_OPTIONS='--experimental-require-module --trace-warnings' nest start",
    "start:dev": "NODE_OPTIONS='--experimental-require-module --trace-warnings' nest start --watch --trace-warnings",
    "start:debug": "NODE_OPTIONS='--experimental-require-module --trace-warnings' nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "NODE_OPTIONS='--experimental-require-module --trace-warnings' node dist/main",
    "start:worker": "NODE_OPTIONS='--experimental-require-module --trace-warnings' nest start --config nest-cli-worker.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "NODE_OPTIONS='--experimental-require-module --trace-warnings' node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "typeorm:entity": "ts-node ./node_modules/.bin/typeorm entity:create",
    "migration:create": "ts-node ./node_modules/.bin/typeorm migration:create",
    "migration:generate": "ts-node ./node_modules/.bin/typeorm migration:generate -d src/database/typeorm.config.ts",
    "migration:up": "ts-node ./node_modules/.bin/typeorm migration:run -d src/database/typeorm.config.ts",
    "migration:down": "ts-node ./node_modules/.bin/typeorm migration:revert -d src/database/typeorm.config.ts",
    "migrate-files": "ts-node src/scripts/migrate-files-from-tce-to-defensoria.ts",
    "geocode-enderecos": "ts-node src/scripts/geocode-enderecos.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org dpero --project centraldevagas-api ./dist && sentry-cli --url https://sentry.defensoria.ro.def.br/ sourcemaps upload --org dpero --project centraldevagas-api ./dist"
  },
